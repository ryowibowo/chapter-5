    <section class="sysreq">
        <div class="container">
            <div class="req-info-top">
                <h5>Can My Computer Run This Game?</h5>
            </div>
            <div class="row">
                <div class="col-lg-6">
                    <h1 class="rq-table-title">System Requirements</h1>
                    <table class="table table-bordered table-content-sys">
                        <tbody>
                            <tr>
                                <td>
                                    <h2 class="requirements-info">OS:</h2>
                                    <p class="requirements-info-content">
                                        Windows 7 64-bit only<br>(No OSX support at this time)
                                    </p>
                                </td>
                                <td>
                                    <h2 class="requirements-info">Processor:</h2>
                                    <p class="requirements-info-content">Intel Core 2 Duo @2.4GHz or AMD Athlon X2 @ 2.8GHz</p>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <h2 class="requirements-info">Memory:</h2>
                                    <p class="requirements-info-content">4 GB RAM</p>
                                </td>
                                <td>
                                    <h2 class="requirements-info">Storage:</h2>
                                    <p class="requirements-info-content">8 GB available space</p>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="2">
                                    <h2 class="requirements-info">Graphics:</h2>
                                    <p class="requirements-info-content">
                                        NVIDIA GeForce GTX 660 2GB or<br>AMD Radeon HD 7850 2GB DirectX11 (Shader Model S)
                                    </p>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </section>

